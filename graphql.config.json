{
	"overwrite": true,
	"schema": "https://graphql.anilist.co",
	"documents": ["src/**/*.graphql"],
	"generates": {
		"src/generated/graphql.ts": {
			"plugins": [
				"typescript",
				"typescript-operations",
				"typescript-react-query"
			]
		}
	},
	"config": {
		"fetcher": {
			"func": "../network/fetchData#fetchData"
		},
		"maybeValue": "T | undefined",
		"skipTypename": true,
		"exposeQueryKeys": true,
		"exposeDocument": true
	}
}
