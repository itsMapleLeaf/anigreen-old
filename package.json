{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "scripts": {
    "dev": "npm-run-all --parallel --print-label dev-*",
    "dev-remix": "remix run",
    "dev-css": "postcss styles --dir app/styles --verbose --watch",
    "dev-codegen": "pnpm run codegen -- --watch",
    "build": "npm-run-all --parallel --print-label build-*",
    "build-remix": "remix build",
    "build-css": "postcss styles --dir app/styles --verbose --env production",
    "start": "remix-serve build",
    "deploy": "flyctl deploy --build-arg REMIX_TOKEN=${REMIX_TOKEN}",
    "codegen": "graphql-codegen --config graphql.config.yml"
  },
  "dependencies": {
