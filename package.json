    "@remix-run/react": "0.17.0",
    "@remix-run/serve": "0.17.0",
    "@tailwindcss/line-clamp": "0.2.0",
    "constate": "3.2.0",
    "date-fns": "2.21.2",
    "dotenv": "9.0.2",
    "focus-visible": "5.2.0",
    "graphql-request": "3.4.0",
    "history": "5.0.0",
    "lodash-es": "4.17.21",
